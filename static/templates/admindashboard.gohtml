      <main id="main-content" class="container mx-auto px-6">
        <div class="flex justify-center py-4" id="appstatus"></div>
        <div hx-get="/appstatus" hx-target="#appstatus" hx-swap="outerHTML" hx-trigger="load" id="appstatus"></div>
            <div class="flex justify-center py-4" id="beteditmatchup">
                <button hx-post="/adminbetedit/matchup" hx-target="#beteditmatchup" hx-swap="outerHTML" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4">
                    View or Edit Matchup Bets
                </button>
            </div>
            <div class="flex justify-center py-4" id="beteditfuture">
                <button hx-post="/adminbetedit/future" hx-target="#beteditfuture" hx-swap="outerHTML" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4">
                    View or Edit Future Bets
                </button>
            </div>
            <div class="flex justify-center py-4" id="beteditprop">
                <button hx-post="/adminbetedit/prop" hx-target="#beteditprop" hx-swap="outerHTML" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4">
                    View or Edit Prop Bets
                </button>
            </div>
            <div class="flex justify-center py-4" id="adminuseredit">
                <button hx-post="/adminuseredit" hx-target="#adminuseredit" hx-swap="outerHTML" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4">
                    View or Edit Users
                </button>
            </div> 
            <div class="flex justify-center py-4" id="admincreatebet">
                <button hx-post="/create-new-bet-form" hx-target="#admincreatebet" hx-swap="outerHTML" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4">
                    Create New Bet
                </button>
            </div> 
            <div class="flex justify-center py-4" id="approve-bets">
                <button hx-post="/userbets/all" hx-target="#approve-bets" hx-swap="outerHTML" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4">
                    Approve or Reject Bets
                </button>
            </div> 
            <div class="flex justify-center py-4" id="grade-bets">
                <button hx-post="/userbets/allgrade" hx-target="#grade-bets" hx-swap="outerHTML" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4">
                    Grade User's Bets
                </button>
            </div>
            <div class="flex justify-center py-4" id="admin-create-userbet">
                <button hx-post="/get-all-bets-admin" hx-target="#admin-create-userbet" hx-swap="outerHTML" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4">
                    Create Bet for User
                </button>
            </div>
        </div>
        
    </main>
    
