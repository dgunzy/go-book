<!DOCTYPE html>
<html>
<head>
    <title>User List</title>
    <style>
        table {
            border-collapse: collapse;
            width: 100%;
        }

        th, td {
            padding: 8px;
            text-align: left;
            border-bottom: 1px solid #ddd;
        }

        tr:hover {background-color: #f5f5f5;}
    </style>
</head>
<body>
    <h1>User List</h1>
    <table>
        <tr>
            <th>User ID</th>
            <th>Username</th>
            <th>Email</th>
            <th>Role</th>
            <th>Balance</th>
            <th>Token</th>
        </tr>
        {{ range . }}
        <tr>
            <td>{{ .UserID }}</td>
            <td>{{ .Username }}</td>
            <td>{{ .Email }}</td>
            <td>{{ .Role }}</td>
            <td>{{ .Balance }}</td>
            <td>{{ .Token }}</td>
        </tr>
        {{ end }}
    </table>
</body>
</html>